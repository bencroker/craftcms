{
  "require": {
    "craftcms/cms": "^3.5",
    "craftcms/postmark": "^2.0",
    "craftcms/redactor": "^2.7",
    "fruitstudios/linkit": "^1.1",
    "marionnewlevant/agnostic-fetch": "^2.0",
    "mmikkel/cp-field-inspect": "^1.2",
    "mmikkel/retcon": "^2.2",
    "nystudio107/craft-retour": "^3.1",
    "nystudio107/craft-scripts": "^1.2",
    "nystudio107/craft-seomatic": "^3.3",
    "nystudio107/craft-templatecomments": "^1.1",
    "ostark/craft-async-queue": "^2.1",
    "putyourlightson/craft-blitz": "^3.6",
    "spacecatninja/imager-x": "^3.1",
    "superbig/craft3-bugsnag": "^2.1",
    "verbb/knock-knock": "^1.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "scripts": {
    "craft-update": [
      "@php craft migrate/all",
      "@php craft project-config/apply",
      "@php craft clear-caches/compiled-templates",
      "@php craft clear-caches/temp-files"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-update-cmd": "@craft-update",
    "post-install-cmd": "@craft-update"
  }
}
